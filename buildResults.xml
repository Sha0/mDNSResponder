<buildResults>
	<dest dir="ExplorerPluginModule_msm">
		<dest dir="Program Files/Bonjour">
			<src file="Clients/ExplorerPlugin/Release/*.dll"/>
		</dest>
	</dest>
	<dest dir="JavaModule_msm">
		<dest dir="Program Files/Bonjour">
			<src file="mDNSWindows/Java/build/prod/dns_sd.jar"/>
		</dest>
		<dest dir="System32">
			<src file="mDNSWindows/Java/build/prod/jdns_sd.dll"/>
		</dest>
	</dest>
	<dest dir="mDNSResponder_msm">
		<dest dir="Program Files/Bonjour">
			<src file="mDNSWindows/SystemService/Release/mDNSResponder.exe"/>
			<src file="mDNSWindows/mdnsNSP/Release/mdnsNSP.dll"/>
		</dest>
		<dest dir="System32">
			<src file="mDNSWindows/DLL/Release/dnssd.dll"/>
			<src file="Clients/DNS-SD.VisualStudio/Release/dns-sd.exe"/>
		</dest>
	</dest>
	<dest dir="PrinterSetupModule_msm">
		<dest dir="Program Files/Bonjour">
			<src file="Clients/PrinterSetupWizard/Release/*.exe"/>
			<src file="Clients/PrinterSetupWizard/Release/*.dll"/>
		</dest>
	</dest>
	<dest dir="sdk_root/Program Files/Bonjour SDK">
		<dest dir="include">
			<src file="mDNSShared/dns_sd.h"/>
		</dest>
		<dest dir="lib">
			<src file="mDNSWindows/DLL/Release/dnssd.lib"/>
		</dest>
		<dest dir="samples/C">
			<src file="Clients/dns-sd.c"/>
			<src file="Clients/DNS-SD.VisualStudio/Release/dns-sd.exe"/>
		</dest>
		<dest dir="samples/Java">
			<src file="Clients/Java/nmakefile"/>
			<src file="Clients/Java/BrowserApp.java"/>
			<src file="Clients/Java/SimpleChat.java"/>
			<src file="Clients/Java/Swing*.java"/>
			<src file="Clients/Java/build/prod/*.jar"/>
		</dest>
	</dest>
</buildResults>
